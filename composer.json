{
    "name": "adelwon/novaposhta",
    "description": "NovaPoshta (API 2.0) for Laravel 7+",
    "keywords": [
        "adelwon",
        "novaposhta",
        "ukraine-delivery",
        "laravel-np",
        "laravel-novaposhta",
        "delivery"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daan",
            "email": "daan@ukr.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Daaner\\NovaPoshta\\Facades\\": "src/Facades",
            "Daaner\\NovaPoshta\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daaner\\NovaPoshta\\NovaPoshtaServiceProvider"
            ],
            "aliases": {
                "NovaPoshta": "Daaner\\NovaPoshta\\Facades\\NovaPoshta"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
